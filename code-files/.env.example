# Azure OpenAI Configuration (Microsoft Foundry)
# Your Azure OpenAI endpoint URL (e.g., https://your-resource.openai.azure.com/)
AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here

# Your Azure OpenAI API key
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here

# The name of your deployed model (e.g., gpt-4, gpt-35-turbo)
AZURE_OPENAI_DEPLOYMENT_NAME=your_deployment_name_here

# Azure AI Search Configuration
# Your Azure AI Search endpoint URL (e.g., https://your-search-service.search.windows.net)
AZURE_SEARCH_ENDPOINT=your_azure_search_endpoint_here

# Your Azure AI Search admin API key
AZURE_SEARCH_API_KEY=your_azure_search_api_key_here

# The name of your search index containing account intelligence
AZURE_SEARCH_INDEX_NAME=your_search_index_name_here
